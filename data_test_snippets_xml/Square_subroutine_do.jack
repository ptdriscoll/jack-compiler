/** Implements a graphical square. */
class Square {

field int x;

   /** Draws the square on the screen. */
   method void draw(int Ax, int Ay) {
      var int y;
      do draw();
      do Screen.drawRectangle(x, y, x + Ax, y + Ay);
   }
}